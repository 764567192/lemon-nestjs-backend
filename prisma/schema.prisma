generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model AcctType {
  id         BigInt   @id(map: "acct_type_pk") @default(autoincrement())
  type       Int      @db.SmallInt
  icon       String   @db.VarChar(255)
  iconName   String   @map("icon_name") @db.VarChar(255)
  sort       BigInt
  createTime DateTime @map("create_time") @db.Timestamptz(6)
  updateTime DateTime @map("update_time") @db.Timestamptz(6)

  @@unique([type, sort], map: "acct_type_pk2")
  @@map("acct_type")
}
